<script>
  var slideIndex = 0;
  opCarousel();

  function opCarousel() {
    var i;
    var x = document.getElementsByClassName("mySlides");
    if (x.length === 0) return; // Verifica si hay elementos con la clase "mySlides"
    for (i = 0; i < x.length; i++) {
      (x[i] as HTMLElement).style.display = "none";
    }
    slideIndex++;
    if (slideIndex > x.length) {
      slideIndex = 1;
    }
    (x[slideIndex - 1] as HTMLElement).style.display = "block";
    setTimeout(opCarousel, 5000);
  }

  var photoIndex = 0;
  carousel();

  function carousel() {
    var j;
    var y = document.getElementsByClassName("myPhotos");
    if (y.length === 0) return; // Verifica si hay elementos con la clase "myPhotos"
    for (j = 0; j < y.length; j++) {
      (y[j] as HTMLElement).style.display = "none";
    }
    photoIndex++;
    if (photoIndex > y.length) {
      photoIndex = 1;
    }
    (y[photoIndex - 1] as HTMLElement).style.display = "block";
    setTimeout(carousel, 5000);
  }
</script>
